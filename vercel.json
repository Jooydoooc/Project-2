{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/books", "destination": "/books.html" },
    { "source": "/unit", "destination": "/unit.html" },
    { "source": "/books/*", "destination": "/books.html" },
    { "source": "/unit/*", "destination": "/unit.html" }
  ]
}
